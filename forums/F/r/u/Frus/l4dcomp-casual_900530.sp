	}
	if (prev)
		RemoveEdict(prev);
	prev = 0;
	while ((ent = FindEntityByClassname(ent, "weapon_rifle_spawn")) != -1)
	{
		if (prev)
			RemoveEdict(prev);
		prev = ent;
	}
	if (prev)
		RemoveEdict(prev);
	for (new i=0;i<10;i++)
		for (new j=0;j<2;j++)
			TierTwoEntID[i][j] = 0;
}
